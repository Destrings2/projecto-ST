main:
  hundred := 100
  readStr(str, hundred)
  spaces := 0
  one := 1
  index := 0
  x := M[index + 0]
  cero := 0
  space := 32
  i := 0

loop:
  if x = cero then goto end
    if x <> space then goto next
      spaces := spaces + one

    next:
      x := M[i + 1]
      i := i + one
      goto loop

end:
  writeInt(spaces)
  halt

equ spaces M[100]
equ hundred M[101]
equ str M[0]
equ x M[102]
equ one M[103]
equ cero M[104]
equ space M[105]
equ i M[106]
equ index M[107]